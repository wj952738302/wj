<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="blacktable" catalog="login" class="bean.Black">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="clasz" catalog="login" class="bean.Clasz">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="student" catalog="login" class="bean.Studnet">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
<table name="user" catalog="login" class="bean.User">
	<primary-key>
		<generator class="native"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>